{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigInfo",
  "type": "object",
  "required": [
    "denom",
    "reward_contract",
    "reward_denom",
    "stake_contract_id",
    "stake_contract_label"
  ],
  "properties": {
    "denom": {
      "type": "string"
    },
    "reward_contract": {
      "type": "string"
    },
    "reward_denom": {
      "type": "string"
    },
    "stake_contract_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "stake_contract_label": {
      "type": "string"
    }
  }
}
