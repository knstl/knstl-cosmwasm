{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "denom",
    "reward_contract",
    "reward_denom",
    "stake_id",
    "stake_label",
    "token_name",
    "token_symbol",
    "tokencontract_id",
    "tokencontract_label"
  ],
  "properties": {
    "denom": {
      "type": "string"
    },
    "reward_contract": {
      "type": "string"
    },
    "reward_denom": {
      "type": "string"
    },
    "stake_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "stake_label": {
      "type": "string"
    },
    "token_name": {
      "type": "string"
    },
    "token_symbol": {
      "type": "string"
    },
    "tokencontract_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "tokencontract_label": {
      "type": "string"
    }
  }
}
